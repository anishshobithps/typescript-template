{
  "name": "typescript-template",
  "version": "1.0.0",
  "description": "A typescript template for my typescript projects.",
  "main": "src/index.js",
  "repository": "git@github.com:Anish-Shobith/typescript-template.git",
  "author": "Anish Shobith P S <anish.shobith19@gmail.com>",
  "engines": {
    "node": ">=16.14.0",
    "npm": "Please use yarn",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "build": "rimraf ./dist && tsc -p .",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "start": "yarn lint:fix && yarn build && node dist/index.js"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "16.11.35",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "prettier": "2.6.2",
    "rimraf": "4.4.1",
    "typescript": "4.6.4"
  }
}
